<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!-- Uncomment this and set the value to your connection's name if you want to 
    use a connection string named something other than "SeekU." -->
    <!--<add key="SeekU.SqlConnectionName" value="Custom-connection-string-name" />-->
  </appSettings>
  <connectionStrings>
    <!-- Put your connection string here -->
    <add name="SeekU" connectionString="Data Source=.\sqlexpress;Initial Catalog=SeekU;Integrated Security=True"/>
  </connectionStrings>
</configuration>